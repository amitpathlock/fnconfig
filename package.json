{
  "name": "fnconfig",
  "version": "0.0.1",
  "license": "MIT",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^4.0.38",
    "eslint": "^9.39.2",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-ava": "^15.1.0",
    "global": "^4.4.0",
    "globals": "^16.5.0"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#pldacappsfnconfig-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#pldacappsfnconfig-display\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "unit-tests": "fiori run --open 'test/unit/unitTests.qunit.html'",
    "int-tests": "fiori run --open 'test/integration/opaTests.qunit.html'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx ."
  },
  "sapuxLayer": "CUSTOMER_BASE",
  "publishConfig": {
    "@amitpathlock:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amitpathlock/fnconfig.git"
  }
}
